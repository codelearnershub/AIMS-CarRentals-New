@model AimsCarRentals.Models.Car
<div class="row">

    <div class="card-header">
        <img src="@WC.ImagePath@Model.CarPictureUrl" style="width:300px" />
    </div>
    <div>
        <ul>
            @Model.Id
            @Model.Name
            @Model.Make
            @Model.PlateNo
            @Model.SerialNo
            @Model.BranchId
            @Model.CategoryId
            @Model.Description
        </ul>
    </div>
</div>